{
  "name": "xstag-pro",
  "version": "3.0.0",
  "description": "Professional Image Steganography Suite with Advanced Encryption",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "lint": "eslint .",
    "security-audit": "npm audit",
    "security-fix": "npm audit fix",
    "build": "echo 'No build required for Node.js backend'",
    "clean": "rm -rf uploads/* temp/* logs/*",
    "backup": "tar -czf backup-$(date +%Y%m%d-%H%M%S).tar.gz uploads/ logs/",
    "stats": "node -e \"console.log('Memory:', process.memoryUsage()); console.log('Uptime:', process.uptime());\"",
    "docker:build": "docker build -t xstag-pro .",
    "docker:run": "docker run -p 3000:3000 -v $(pwd)/uploads:/app/uploads -v $(pwd)/logs:/app/logs xstag-pro"
  },
  "keywords": [
    "steganography",
    "encryption",
